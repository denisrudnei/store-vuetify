{
  "name": "shop",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register server/index.ts --transpileOnly",
    "build": "nuxt build && tsc -p tsconfig-build.json",
    "start": "node -r tsconfig-paths/register dist/server/index.js",
    "heroku-postbuild": "npm run build",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore . --fix",
    "lint": "npm run lint:js",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/google-gtag": "^1.0.4",
    "@nuxtjs/pwa": "^3.3.5",
    "apollo-server-express": "^2.25.2",
    "consola": "^2.15.3",
    "core-js": "^3.9.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "graphql-tag": "^2.12.5",
    "nuxt": "^2.15.7",
    "pg": "^8.6.0",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.34",
    "vue-the-mask": "^0.11.1",
    "vuetify": "^2.5.6",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@nuxt/types": "^2.15.7",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/vuetify": "^1.12.1",
    "@types/consola": "^2.2.5",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.1",
    "@types/faker": "^5.5.6",
    "@types/vue-the-mask": "^0.11.1",
    "@vue/test-utils": "^1.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.7.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "vue-jest": "^3.0.4"
  }
}
